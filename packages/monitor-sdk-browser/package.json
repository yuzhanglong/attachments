    "@babel/runtime": "^7.16.0",
    "@types/lodash": "^4.14.172",
    "babel-loader": "^8.2.2",
    "babel-plugin-lodash": "^3.3.4",
    "html-webpack-plugin": "^5.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "terser-webpack-plugin": "^5.2.4",
    "webpack": "^5.61.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-dev-server": "^4.4.0"
